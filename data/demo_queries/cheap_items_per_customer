SELECT {
  ?customer cheapItems _:cheapItems ;
            name       ?name        ;
            surname    ?surname     .

  _:cheapItems item ?item .

  ?item name  ?itemName  ;
        price ?itemPrice .
}
WHERE {
  ?customer -12/14 ?order .
  ?order -42/40 ?item .
  ?item 56 ?itemName ;
        57 ?itemPrice .
  FILTER (?itemPrice <= 150)
}
